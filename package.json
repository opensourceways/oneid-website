{
  "name": "openeulercenter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:euler": "pnpm --filter euler dev",
    "build:euler": "pnpm --filter euler build",
    "dev:gauss": "pnpm --filter gauss dev",
    "build:gauss": "pnpm --filter gauss build",
    "dev:mindspore": "pnpm --filter mindspore dev",
    "build:mindspore": "pnpm --filter mindspore build",
    "build": "pnpm build:euler & pnpm build:gauss",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,ts,vue,jsx,tsx}\"",
    "fix": "eslint --fix \"src/**/*.{js,ts,vue,jsx,tsx}\"",
    "lint:components": "eslint \"opendesign/**/*.{js,ts,vue,jsx,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@authing/web": "^5.1.2",
    "authing-js-sdk": "^4.23.35",
    "axios": "^1.1.3",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "element-plus": "^2.2.17",
    "jsencrypt": "^3.3.2",
    "normalize.css": "^8.0.1",
    "opendesign": "./opendesign",
    "pinia": "^2.0.22",
    "rxjs": "^7.5.7",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-icons": "^0.14.10",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.40.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue,jsx,tsx}": [
      "pnpm lint"
    ]
  }
