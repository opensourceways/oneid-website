{
  "name": "openeulercenter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:euler": "pnpm --filter euler dev",
    "test:euler": "pnpm --filter euler test",
    "build:euler": "pnpm --filter euler build",
    "dev:gauss": "pnpm --filter gauss dev",
    "test:gauss": "pnpm --filter gauss test",
    "build:gauss": "pnpm --filter gauss build",
    "dev:mindspore": "pnpm --filter mindspore dev",
    "build:mindspore": "pnpm --filter mindspore build",
    "dev:merlin": "pnpm --filter merlin dev",
    "test:merlin": "pnpm --filter merlin test",
    "build:merlin": "pnpm --filter merlin build",
    "build": "pnpm build:euler & pnpm build:gauss",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,ts,vue,jsx,tsx}\"",
    "fix": "eslint --fix \"src/**/*.{js,ts,vue,jsx,tsx}\"",
    "lint:components": "eslint \"opendesign/**/*.{js,ts,vue,jsx,tsx}\"",
